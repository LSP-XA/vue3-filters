    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "husky": "^4.2.3",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "less-vars-to-js": "^1.3.0",
